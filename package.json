{
  "name": "fithou-bot",
  "version": "0.0.1",
  "description": "Fithou bot",
  "main": "index.js",
  "scripts": {
    "copy-env-files": "copyfiles ./.env.* dist/",
    "build": "rm -rf ./dist && tsc && npx tsc-alias",
    "lint": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "./node_modules/.bin/eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "npx husky install",
    "start": "NODE_PATH=./dist node dist/server.js",
    "start-ts": "NODE_PATH=./src nodemon src/server.ts",
    "start-dev": "NODE_ENV=dev npm run start",
    "start-stg": "NODE_ENV=stg npm run start",
    "start-prd": "NODE_ENV=prd npm run start"
  },
  "pre-commit": [
    "lint-fix"
  ],
  "author": "",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/cheerio": "^0.22.31",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/md5": "^2.3.2",
    "@types/multer": "^1.4.7",
    "@types/node-schedule": "^2.1.0",
    "@types/validator": "^13.7.1",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-winston": "^4.2.0",
    "helmet": "^4.6.0",
    "md5": "^2.3.0",
    "mongoose": "^6.0.12",
    "multer": "^1.4.3",
    "nocache": "^3.0.1",
    "node-html-to-image": "^3.2.4",
    "node-schedule": "^2.1.0",
    "sequelize": "^6.8.0",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  }
}
